  },
  "keywords": [
    "boilerplate",
    "react",
    "redux",
    "scaffold",
    "skeleton",
    "starter",
    "webpack"
  ],
  "license": "MIT",
  "name": "rak",
  "repository": {
    "url": "git@github.com:bjacobel/rak.git",
    "type": "git"
  },
  "scripts": {
    "awsUtils": "node aws/utils",
    "lint": "eslint . && stylelint src/stylesheets/*.css",
    "start": "webpack-dev-server",
    "test": "jest --coverage && ./install-test.sh",
    "webpack": "webpack -pc"
  },
  "version": "0.0.2"
}
